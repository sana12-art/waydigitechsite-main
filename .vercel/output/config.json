{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/__ecom/checkout/?$",
			"dest": "_render"
		},
		{
			"src": "^/_api(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/_paylink/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_wix/extensions/backoffice/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_wix/extensions/service-plugins/([^/]+?)(?:/(.*?))?/?$",
			"dest": "_render"
		},
		{
			"src": "^/_wix/extensions/webhooks/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/callback/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout-callback/?$",
			"dest": "_render"
		},
		{
			"src": "^/checkout/?$",
			"dest": "_render"
		},
		{
			"src": "^/robots\\.txt/?$",
			"dest": "_render"
		},
		{
			"src": "^(?:/(.*?))?/?$",
			"dest": "_render"
		}
	]
}